#!groovy

properties([disableConcurrentsBuils()])

pipeline {
  agent {
    label 'master'
  }
  options {
    buildDiscarder( logRotator( numToKeepStr: '10', artifactNumToKeepStr: '10') )
    timestamps()
  }
  stages {
    stage( "Request hostname" ){
      steps {
        sh 'ssh worker-node1 \'hostname\''
      }
    }
  }
}
