#!groovy

properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'master'
  }
  options {
    buildDiscarder( logRotator( numToKeepStr: '7', artifactNumToKeepStr: '7') )
    timestamps()
  }
  stages {
    stage( "Request hostname" ){
      steps {
        sh whoami
        //sh -c 'ssh localhost \'hostname\''
      }
    }
    stage( "Check exist ansible" ){
      steps {
        sh -c 'ansible --version'
      }
    }
  }
}
