#!groovy

properties([disableConcurrentBuilds()])

pipeline {
  agent {
    label 'master'
  }
  options {
    buildDiscarder( logRotator( numToKeepStr: '10', artifactNumToKeepStr: '10') )
    timestamps()
  }
  stages {
    stage( "Request hostname" ){
      steps {
        sh 'ssh worker-node1 \'hostname\''
      }
    }
  }
  stages {
    stage( "Check exist ansible" ){
      steps {
        sh 'ansible --version'
      }
    }
  }
}
